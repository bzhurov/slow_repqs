\begin_layout Part*
Appendix 1
\end_layout

\begin_layout Wide Text
\begin_inset Formula 
\begin{align}
\frac{dM_{1}}{dt} & =r_{m}G_{1}-d_{m}M_{1}\label{cmM1}\\
\frac{dM_{2}}{dt} & =r_{m}G_{2}-d_{m}M_{2}\label{cmM2}\\
\frac{dM_{3}}{dt} & =r_{m}\left(G_{3}+\omega(g_{t}-G_{3I})\right)-d_{m}M_{3}\label{cmM3}\\
\frac{dM_{I}}{dt} & =r_{m}^{I}G_{I}-d_{m}M_{I}\label{cmMI}\\
\notag\\
\frac{dP_{1}}{dt} & =r_{p}M_{1}+2k_{ud}D_{1}-2k_{d}P_{1}^{2}-d_{p}P_{1}\label{cmP1}\\
\frac{dP_{2}}{dt} & =r_{p}M_{2}+2k_{ud}D_{2}-2k_{d}P_{2}^{2}-d_{p}P_{2}\label{cmP2}\\
\frac{dP_{3}}{dt} & =r_{p}M_{3}+2k_{ud}D_{3}-2k_{d}P_{3}^{2}-d_{p}P_{3}\label{cmP3}\\
\frac{dP_{I}}{dt} & =r_{p}M_{I}-d_{p}P_{I}\label{cmPI}\\
\notag\\
\frac{dS}{dt} & =r_{s}P_{I}-(d_{s}+\eta)S+k_{ua}R-k_{a}p_{R}S\label{cmS}\\
\frac{dR}{dt} & =-k_{ua}R+k_{a}p_{R}S-2k_{Rd}R^{2}+2k_{Rud}D_{R}\label{cmR}\\
\notag
\end{align}

\end_inset


\begin_inset Formula 
\begin{align}
\frac{dD_{1}}{dt} & =k_{d}P_{1}^{2}-k_{ud}D_{1}-k_{r}D_{1}G_{2}+k_{ur}(g_{t}-G_{2})-k_{r}D_{1}G_{I}+k_{ur}(g_{t}-G_{I})-\delta d_{p}D_{1}\label{cmD1}\\
\frac{dD_{2}}{dt} & =k_{d}P_{2}^{2}-k_{ud}D_{2}-k_{r}D_{2}G_{3}+k_{ur}(g_{t}-G_{3})-\delta d_{p}D_{2}\label{cmD2}\\
\frac{dD_{3}}{dt} & =k_{d}P_{3}^{2}-k_{ud}D_{3}-k_{r}D_{3}G_{1}+k_{ur}(g_{t}-G_{1})-\delta d_{p}D_{3}\label{cmD3}\\
\frac{dD_{R}}{dt} & =k_{Rd}R^{2}-k_{Rud}D_{R}-k_{Rr}G_{3I}D_{aiR}+k_{Rur}(g_{t}-G_{3I})-\delta d_{p}D_{R}\label{cmDR}\\
\notag\\
\frac{dG_{1}}{dt} & =-k_{r}G_{1}D_{3}+k_{ur}(g_{t}-G_{1})\label{cmG1}\\
\frac{dG_{2}}{dt} & =-k_{r}G_{2}D_{1}+k_{ur}(g_{t}-G_{2})\label{cmG2}\\
\frac{dG_{I}}{dt} & =-k_{r}G_{I}D_{1}+k_{ur}(g_{t}-G_{I})\label{cmGI}\\
\frac{dG_{3}}{dt} & =-k_{r}G_{3}D_{2}+k_{ur}(g_{t}-G_{3})\label{cmG3}\\
\frac{dG_{3I}}{dt} & =-k_{Rr}G_{3I}D_{R}+k_{Rur}(g_{t}-G_{3I})\label{cmG3I}
\end{align}

\end_inset


\end_layout

\begin_layout Standard
Reactions (
\begin_inset CommandInset ref
LatexCommand ref
reference "cmD1"

\end_inset

) - (
\begin_inset CommandInset ref
LatexCommand ref
reference "cmDR"

\end_inset

), (
\begin_inset CommandInset ref
LatexCommand ref
reference "cmG1"

\end_inset

) - (
\begin_inset CommandInset ref
LatexCommand ref
reference "cmG3I"

\end_inset

) and (
\begin_inset CommandInset ref
LatexCommand ref
reference "cmR"

\end_inset

) is assumed to be fast.
 Then we can write: 
\begin_inset Formula 
\begin{align}
D_{i} & =\frac{k_{d}P_{i}^{2}}{\delta d_{p}+k_{ud}},\ i=\overline{1,3}\label{vcDiDim}\\
G_{i} & =\frac{k_{ur}\cdot g_{t}}{\frac{k_{r}\cdot k_{d}}{\delta\cdot d_{p}+k_{ud}}P_{k}^{2}+k_{ur}}\label{vcGiDim}\\
R & =\frac{k_{a}p_{R}S}{k_{ua}}\label{vcRDim}\\
D_{R} & =\frac{k_{Rd}R^{2}}{\delta d_{p}+k_{Rud}}\label{vcDRDim}\\
G_{3I} & =\frac{k_{Rur}g_{t}}{\frac{k_{Rr}k_{Rd}}{\delta d_{p}+k_{Rud}}R^{2}+k_{Rur}}\label{vcDR}
\end{align}

\end_inset


\end_layout

\begin_layout Standard
denote 
\begin_inset Formula $\sigma=\frac{k_{d}}{\delta\cdot d_{p}+k_{ud}}$
\end_inset

, 
\begin_inset Formula $\kappa=k_{r}/k_{ur}$
\end_inset

, 
\begin_inset Formula $\kappa_{a}=\frac{k_{a}p_{R}}{k_{ua}}$
\end_inset

, 
\begin_inset Formula $\sigma_{R}=\frac{k_{Rd}}{\delta d_{p}+k_{Rud}}$
\end_inset

 and 
\begin_inset Formula $\kappa_{R}=\frac{k_{Rr}}{k_{Rur}}$
\end_inset

.
 Then
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
D_{i} & =\sigma P_{i}^{2}\\
G_{i} & =\frac{g_{t}}{\sigma\kappa P_{k}^{2}+1}\\
R & =\kappa_{a}S\\
D_{R} & =\sigma_{R}\kappa_{a}^{2}S^{2}\\
G_{3I} & =\frac{g_{t}}{\sigma_{R}\kappa_{R}\kappa_{a}^{2}S^{2}+1}
\end{align}

\end_inset


\end_layout

\begin_layout Standard
For each gene from base repressilator ring consider total protein concentration
 
\begin_inset Formula $n_{i}$
\end_inset

, as slow variable, also as slow variable we can write total AI(small molecule)
 concentration 
\begin_inset Formula $n_{s}$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
n_{i} & =P_{i}+2D_{i}+2(g_{t}-G_{k}),\ i=2,3,\ k=3,1\\
n_{1} & =P_{1}+2D_{1}+2(g_{t}-G_{2})+2(g_{t}-G_{I})\\
n_{s} & =S+R+2D_{R}+2(g_{t}-G_{3I})
\end{align}

\end_inset


\end_layout

\begin_layout Standard
Obviosly derivation of new variables is 
\begin_inset Formula 
\begin{align}
\dot{n_{i}} & =r_{p}M_{i}-d_{p}P_{i}-\delta d_{p}D_{i},\ i=\overline{1,3}\label{phDni}\\
\dot{n_{s}} & =r_{s}P_{I}-(d_{s}+\eta)S-\delta d_{p}D_{R}\label{phDns}
\end{align}

\end_inset


\end_layout

\begin_layout Wide Text
Since 
\begin_inset Formula $\dfrac{dD_{i}}{dt}=\dfrac{dG_{i}}{dt}=\dfrac{dR}{dt}=0$
\end_inset

, 
\begin_inset Formula 
\begin{align}
\dot{n_{1}} & =\dfrac{\partial n_{1}}{\partial P_{1}}\frac{dP_{1}}{dt}=\left(1+4\sigma P_{1}+8g_{t}\kappa\sigma\frac{P_{1}}{(\sigma\kappa P_{1}^{2}+1)^{2}}\right)\frac{dP_{1}}{dt}=\Pi_{1}(P_{1})\frac{dP_{1}}{dt}\label{rDni}\\
\dot{n_{i}} & =\dfrac{\partial n_{i}}{\partial P_{i}}\frac{dP_{i}}{dt}=\left(1+4\sigma P_{i}+4g_{t}\kappa\sigma\frac{P_{i}}{(\sigma\kappa P_{i}^{2}+1)^{2}}\right)\frac{dP_{i}}{dt}=\Pi(P_{i})\frac{dP_{i}}{dt}\\
 & \text{for}\ i=2,3\\
\dot{n_{s}} & =\dfrac{\partial n_{s}}{\partial S}\frac{dS}{dt}=\left((1+\kappa_{a})+4\sigma_{R}\kappa_{a}^{2}S+4g_{t}\sigma_{R}\kappa_{R}\kappa_{a}^{2}\frac{S}{(\sigma_{R}\kappa_{R}\kappa_{a}^{2}S^{2}+1)^{2}}\right)\frac{dS}{dt}=\Pi_{I}(S)\dfrac{dS}{dt}\label{rDns}
\end{align}

\end_inset


\end_layout

\begin_layout Wide Text
Finaly we can construct 
\begin_inset Quotes eld
\end_inset

true reducted
\begin_inset Quotes eld
\end_inset

 model:
\end_layout

\begin_layout Wide Text
\begin_inset Formula 
\begin{align}
\frac{dM_{1}}{dt} & =r_{m}\frac{g_{t}}{\sigma\kappa P_{3}^{2}+1}-d_{m}M_{1}\\
\frac{dM_{2}}{dt} & =r_{m}\frac{g_{t}}{\sigma\kappa P_{1}^{2}+1}-d_{m}M_{2}\\
\dfrac{dM_{3}}{dt} & =r_{m}\left(\frac{g_{t}}{\sigma\kappa P_{2}^{2}+1}+\omega\left(g_{t}-\frac{g_{t}}{\sigma_{R}\kappa_{R}\kappa_{a}^{2}S^{2}+1}\right)\right)-d_{m}M_{3}\\
\frac{dM_{I}}{dt} & =r_{m}^{I}\frac{g_{t}}{\sigma\kappa P_{1}^{2}+1}-d_{m}M_{I}\\
\notag\\
\dfrac{dP_{1}}{dt}\Pi_{1}(P_{1}) & =r_{p}M_{1}-d_{p}P_{1}-\delta d_{p}\sigma P_{1}^{2}\\
\dfrac{dP_{2}}{dt}\Pi(P_{2}) & =r_{p}M_{2}-d_{p}P_{2}-\delta d_{p}\sigma P_{2}^{2}\\
\dfrac{dP_{3}}{dt}\Pi(P_{3}) & =r_{p}M_{3}-d_{p}P_{3}-\delta d_{p}\sigma P_{3}^{2}\\
\notag\\
\frac{dP_{I}}{dt} & =r_{p}M_{I}-d_{p}P_{I}\\
\dfrac{dS}{dt}\Pi_{I}(S) & =r_{s}P_{I}-(d_{s}+\eta)S-\delta d_{p}\sigma_{R}\kappa_{a}^{2}S^{2}
\end{align}

\end_inset


\end_layout

