\begin_layout Standard
Математическая модель схемы строится на основе закона действующих масс,
 при этом реакции транскрипции, трансляции, диффузии и производства сигнальной
 молекулы считаются линейными.
 Нелинейными же являются процессы димеризации, 
\color blue
посадки ТФ на промотор и объединения молекулы 
\begin_inset Formula $S$
\end_inset


\color inherit
 с белком в комплекс.
 В уравнении 
\begin_inset CommandInset ref
LatexCommand ref
reference "cmM3-1"

\end_inset

 введён искусственный параметр 
\begin_inset Formula $\omega$
\end_inset

 который описывает отношение между скоростями транскрипций генов 
\begin_inset Formula $G_{3}$
\end_inset

 и 
\begin_inset Formula $G_{3I}$
\end_inset

.
 
\color green
(Нужны дополнительные пояснения про 
\begin_inset Formula $\omega$
\end_inset

?)
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\frac{dM_{1}}{dt} & =r_{m}G_{1}-d_{m}M_{1}\label{cmM1-1}\\
\frac{dM_{2}}{dt} & =r_{m}G_{2}-d_{m}M_{2}\label{cmM2-1}\\
\frac{dM_{3}}{dt} & =r_{m}\left(G_{3}+\omega(g_{t}-G_{3I})\right)-d_{m}M_{3}\label{cmM3-1}\\
\frac{dM_{I}}{dt} & =r_{m}^{I}G_{I}-d_{m}M_{I}\label{cmMI-1}\\
\notag\\
\frac{dP_{1}}{dt} & =r_{p}M_{1}+2k_{ud}D_{1}-2k_{d}P_{1}^{2}-d_{p}P_{1}\label{cmP1-1}\\
\frac{dP_{2}}{dt} & =r_{p}M_{2}+2k_{ud}D_{2}-2k_{d}P_{2}^{2}-d_{p}P_{2}\label{cmP2-1}\\
\frac{dP_{3}}{dt} & =r_{p}M_{3}+2k_{ud}D_{3}-2k_{d}P_{3}^{2}-d_{p}P_{3}\label{cmP3-1}\\
\frac{dP_{I}}{dt} & =r_{p}M_{I}-d_{p}P_{I}\label{cmPI-1}\\
\notag\\
\frac{dS}{dt} & =r_{s}P_{I}-d_{s}\text{S}-\eta(S-S_{e})+k_{ua}R-k_{a}p_{R}S\label{cmS-1}\\
\frac{dR}{dt} & =-k_{ua}R+k_{a}p_{R}S-2k_{Rd}R^{2}+2k_{Rud}D_{R}\label{cmR-1}\\
\notag
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\frac{dD_{1}}{dt} & =k_{d}P_{1}^{2}-k_{ud}D_{1}-k_{r}D_{1}G_{2}+k_{ur}(g_{t}-G_{2})\label{cmD1-1}\\
 & \ \ \ \ \ -k_{r}D_{1}G_{I}+k_{ur}(g_{t}-G_{I})-\delta d_{p}D_{1}\\
\frac{dD_{2}}{dt} & =k_{d}P_{2}^{2}-k_{ud}D_{2}-k_{r}D_{2}G_{3}+k_{ur}(g_{t}-G_{3})-\delta d_{p}D_{2}\label{cmD2-1}\\
\frac{dD_{3}}{dt} & =k_{d}P_{3}^{2}-k_{ud}D_{3}-k_{r}D_{3}G_{1}+k_{ur}(g_{t}-G_{1})-\delta d_{p}D_{3}\label{cmD3-1}\\
\frac{dD_{R}}{dt} & =k_{Rd}R^{2}-k_{Rud}D_{R}-k_{Rr}G_{3I}D_{aiR}+k_{Rur}(g_{t}-G_{3I})-\delta d_{p}D_{R}\label{cmDR-1}\\
\notag\\
\frac{dG_{1}}{dt} & =-k_{r}G_{1}D_{3}+k_{ur}(g_{t}-G_{1})\label{cmG1-1}\\
\frac{dG_{2}}{dt} & =-k_{r}G_{2}D_{1}+k_{ur}(g_{t}-G_{2})\label{cmG2-1}\\
\frac{dG_{I}}{dt} & =-k_{r}G_{I}D_{1}+k_{ur}(g_{t}-G_{I})\label{cmGI-1}\\
\frac{dG_{3}}{dt} & =-k_{r}G_{3}D_{2}+k_{ur}(g_{t}-G_{3})\label{cmG3-1}\\
\frac{dG_{3I}}{dt} & =-k_{Rr}G_{3I}D_{R}+k_{Rur}(g_{t}-G_{3I})\label{cmG3I-1}
\end{align}

\end_inset


\end_layout

